name = "Varya"
age = 18
city = "Moscow"
print('Меня зовут',name)
print('Мой возраст',age)
print('Мой город',city)

num1 = int(input("Введите первое число: "))
num2 = int(input("Введите второе число: "))
sum_result = num1 + num2
multiply_result = num1 * num2
print("Результат сложения: ", sum_result)
print("Результат умножения: ", multiply_result)

favorite_color = "blue"
print("Мой любимый цвет: ", favorite_color)

mood = ("good")
print("Сегодня я чувствую себя: ", mood)

string = input("Введите любую строку: ")
print("Длина строки: ", len(string))

favorite_number = 52
print("Моё любимое число: ", favorite_number)

celsius = float(input("Введите температуру в градусах Цельсия: "))
print("Температура в градусах Цельсия: ", celsius)
fahrenheit = celsius * 9/5 + 32
print("Температура в градусах Фаренгейта: ", fahrenheit)

word = input("Введите любое слово: ")
print(word*3)

name = "Varya"
print("Имя в верхнем регистре: ", name.upper())
print("Имя в нижнем регистре: ", name.lower())

num_as_string = input("Введите число: ")
num_as_integer = int(num_as_string)
print("Результат: ", num_as_integer)

